#include <iostream>
using namespace std;

void mostrar(float *arr, int tamanio){
for(int i=0;i<tamanio;i++){
cout<<arr[i]<<" ";
}}

void sumar(float *v,int tamanio){
float suma = 0;
for(int i=0;i<tamanio;i++){
  suma += v[i];
}cout<<suma;}

int main(){
float vector[]={1.2,2.3,4.5};
mostrar(vector,3);
cout<<endl;
sumar(vector,3);
return 0 ;
}
